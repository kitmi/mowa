{
  "name": "mowa",
  "version": "1.0.0",
  "description": "A modular web application framework based on koa.",
  "scripts": {
    "doc": "jsdoc -c jsdoc.json -t ./node_modules/ink-docstrap/template -R README.md -d './docs/api-doc' -r --verbose ./src",
    "jison": "jison ./src/oolong/lang/grammar/oolong.jison -o ./src/oolong/lang/oolong.js",
    "test": "npm run build-dev && mocha -R spec -t 5000 test/*.mocha.js",
    "cover": "npm run build-dev && nyc --reporter=html --reporter=text mocha test/*.mocha.js",
    "travis": "npm run build-dev && nyc --reporter=text-lcov mocha test/*.mocha.js",
    "build": "NODE_ENV=production babel src -d dist --copy-files",
    "build-dev": "BABEL_ENV=development babel src -d temp --copy-files"
  },
  "keywords": [
    "web",
    "builder",
    "modular",
    "framework",
    "koa"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kitmi/mowa.git"
  },
  "bin": {
    "mowa": "./bin/mowa.js"
  },
  "bugs": {
    "url": "https://github.com/kitmi/mowa/issues"
  },
  "license": "MIT",
  "contributors": [
    {
      "name": "Rockie Guo",
      "email": "rockie@kitmi.com.au",
      "url": "https://github.com/rockie"
    }
  ],
  "devDependencies": {
    "archiver": "^2.1.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-contracts": "^1.2.0",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.0",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.5.5",
    "mocha": "^3.2.0",
    "nyc": "^11.3.0",
    "should": "^8.0.2",
    "supertest": "^3.0.0",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "webpack": "^3.8.1",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "inflection": "^1.12.0",
    "inquirer": "^3.0.6",
    "koa": "^2.4.1",
    "koa-better-body": "^3.0.4",
    "koa-compress": "^2.0.0",
    "koa-csrf": "^3.0.6",
    "koa-error": "^3.1.0",
    "koa-etag": "^3.0.0",
    "koa-flash-message": "^0.1.6",
    "koa-generic-session-mongo": "^0.4.0",
    "koa-methodoverride": "^2.0.0",
    "koa-mount": "^3.0.0",
    "koa-mysql-session": "0.0.2",
    "koa-pg-session": "^2.0.1",
    "koa-redis": "^3.1.1",
    "koa-router": "^7.3.0",
    "koa-session-minimal": "^3.0.4",
    "koa-sqlite3-session": "^1.0.3",
    "koa-static": "^4.0.2",
    "koa-views": "^6.1.3",
    "lru-cache": "^2.7.3",
    "moment": "^2.10.6",
    "moment-timezone": "^0.4.1",
    "mongodb": "^2.1.4",
    "mysql2": "^1.5.1",
    "nodemiral": "^1.1.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pg": "^4.4.6",
    "pg-hstore": "^2.3.2",
    "randomstring": "^1.1.5",
    "rk-config": "^1.0.0",
    "rk-utils": "^2.0.0",
    "shelljs": "^0.7.7",
    "silent-npm-registry-client": "^2.1.0",
    "socket.io": "^1.4.8",
    "sqlite3": "^3.1.1",
    "tedious": "^1.13.2",
    "tiny-xml": "^1.0.6",
    "topo-sort": "^1.0.0",
    "validator": "^7.0.0",
    "winston": "^2.4.0",
    "winston-daily-rotate-file": "^1.7.2",
    "winstonflight": "^1.0.1"
  },
  "main": "index",
  "engines": {
    "node": "7.6.0"
  }
}
