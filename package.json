{
  "name": "mowa",
  "version": "1.0.10",
  "description": "A modular web application framework based on koa.",
  "scripts": {
    "doc": "jsdoc -c jsdoc.json -t ./node_modules/ink-docstrap/template -R README.md -d './docs/api-doc' -r --verbose ./src",
    "jison": "jison ./src/oolong/lang/grammar/oolong.jison -o ./src/oolong/lang/oolong.js",
    "test:only": "mocha test/*.mocha.js --timeout 5000",
    "test": "npm run build:test && npm run test:only",
    "cover": "nyc --reporter=html --reporter=text mocha test/*.mocha.js --timeout 5000",
    "travis": "npm run build:test && nyc --reporter=lcovonly mocha test/*.mocha.js --timeout 5000 && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "bulid:clean": "rm -rf lib",
    "build": "npm run bulid:clean && NODE_ENV=production babel src -d lib --copy-files",
    "build:test": "npm run bulid:clean && NODE_ENV=test babel src -d lib --copy-files && node test/prepare-for-test",
    "build:dev": "npm run bulid:clean && NODE_ENV=development babel src -d lib --copy-files",
    "build:watch": "NODE_ENV=development babel src -w -d lib --copy-files"
  },
  "keywords": [
    "web",
    "builder",
    "modular",
    "framework",
    "koa"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kitmi/mowa.git"
  },
  "bin": {
    "mowa": "./bin/mowa.js"
  },
  "bugs": {
    "url": "https://github.com/kitmi/mowa/issues"
  },
  "license": "MIT",
  "contributors": [
    {
      "name": "Rockie Guo",
      "email": "rockie@kitmi.com.au",
      "url": "https://github.com/rockie"
    }
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-plugin-contract": "^1.3.0",
    "coveralls": "^3.0.0",
    "ink-docstrap": "^1.3.2",
    "jison": "^0.4.18",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "should": "^8.0.2",
    "superagent": "^4.0.0-beta.5",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "archiver": "^2.1.0",
    "compare-versions": "^3.1.0",
    "escodegen": "^1.9.0",
    "hyperid": "^1.4.1",
    "inquirer": "^3.0.6",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.1",
    "koa-compress": "^2.0.0",
    "koa-connect": "^2.0.1",
    "koa-csrf": "^3.0.6",
    "koa-error": "^3.2.0",
    "koa-etag": "^3.0.0",
    "koa-flash-message": "^0.1.6",
    "koa-methodoverride": "^2.0.0",
    "koa-mount": "^3.0.0",
    "koa-passport": "^4.0.1",
    "koa-router": "^7.3.0",
    "koa-session-memory": "^1.0.2",
    "koa-session-mowa": "^5.9.1",
    "koa-static": "^4.0.2",
    "koa-views": "^6.1.3",
    "koa-virtual-host": "^0.2.0",
    "lru-cache": "^2.7.3",
    "moment": "^2.22.0",
    "moment-timezone": "^0.4.1",
    "node-ssh": "^5.1.0",
    "number-to-letter": "^1.0.1",
    "pluralize": "^7.0.0",
    "randomstring": "^1.1.5",
    "rk-config": "^1.0.3",
    "rk-utils": "^2.1.4",
    "shelljs": "^0.7.7",
    "silent-npm-registry-client": "^2.1.0",
    "socket.io": "^2.1.1",
    "swig-templates": "^2.0.2",
    "tiny-xml": "^1.0.6",
    "uniqid": "^5.0.3",
    "uuid": "^3.3.2",
    "validator": "^9.4.1",
    "webpack": "^4.17.1",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-merge": "^4.1.4",
    "winston": "^2.4.0",
    "winston-daily-rotate-file": "^1.7.2",
    "winstonflight": "^1.0.2"
  },
  "main": "index",
  "engines": {
    "node": "8.11.4"
  }
}
