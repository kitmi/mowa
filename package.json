{
  "name": "mowa",
  "version": "0.0.6",
  "description": "A modular web application framework based on koa.",
  "scripts": {
    "doc": "./node_modules/.bin/jsdoc -c jsdoc.json -t ./node_modules/ink-docstrap/template -R README.md -d './docs/api-doc' -r --verbose ./lib",
    "jison": "jison ./lib/oolong/lang/grammar/oolong.jison -o ./lib/oolong/lang/oolong.js",
    "test": "node_modules/.bin/mocha -R spec -t 5000 test/*.mocha.js",
    "cover": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -R dot -t 5000 test/*.mocha.js",
    "travis": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha --report lcovonly -- -R dot -t 5000 test/*.mocha.js"
  },
  "keywords": [
    "web",
    "builder",
    "modular",
    "framework",
    "koa"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kitmi/mowa.git"
  },
  "bin": {
    "mowa": "./bin/mowa.js"
  },
  "bugs": {
    "url": "https://github.com/kitmi/mowa/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/kitmi/mowa/raw/master/LICENSE"
    }
  ],
  "contributors": [
    {
      "name": "Rockie Guo",
      "email": "rockie@kitmi.com.au",
      "url": "https://github.com/rockie"
    }
  ],
  "devDependencies": {
    "ink-docstrap": "^1.3.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.4.3",
    "mocha": "^3.2.0",
    "should": "^8.0.2",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "dot": "^1.0.3",
    "inflection": "^1.12.0",
    "inquirer": "^3.0.6",
    "koa": "1.1.2",
    "koa-async": "^1.0.0",
    "koa-better-body": "^1.0.17",
    "koa-compress": "^1.0.9 ",
    "koa-etag": "^2.1.0",
    "koa-favicon": "^1.2.0",
    "koa-flash": "^1.0.0",
    "koa-generic-session": "^1.10.0",
    "koa-methodoverride": "1.0.0",
    "koa-middleware-swig": "^1.0.0",
    "koa-mount": "^1.3.0",
    "koa-passport": "^1.3.0",
    "koa-router": "^5.3.0",
    "koa-static": "^1.5.2",
    "koa-vhost": "^0.5.0",
    "koa-webpack-dev": "^1.0.0",
    "lru-cache": "^2.7.3",
    "manage-database": "^0.1.3",
    "moment": "^2.10.6",
    "moment-timezone": "^0.4.1",
    "mongodb": "^2.1.4",
    "mysql-crud": "^0.1.4",
    "node-mysql-promise2": "*",
    "nodemiral": "^1.1.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pg": "^4.4.6",
    "pg-hstore": "^2.3.2",
    "randomstring": "^1.1.5",
    "regenerator": "^0.8.46",
    "rk-config": "^1.0.0",
    "rk-utils": "^1.0.1",
    "shelljs": "^0.7.7",
    "silent-npm-registry-client": "^2.1.0",
    "socket.io": "^1.4.8",
