{
  "name": "mowa",
  "version": "1.0.4",
  "description": "A modular web application framework based on koa.",
  "scripts": {
    "doc": "jsdoc -c jsdoc.json -t ./node_modules/ink-docstrap/template -R README.md -d './docs/api-doc' -r --verbose ./src",
    "jison": "jison ./src/oolong/lang/grammar/oolong.jison -o ./src/oolong/lang/oolong.js",
    "test": "npm run build-test && mocha -R spec -t 5000 test/*.mocha.js",
    "cover": "npm run build-test && nyc --reporter=html --reporter=text mocha test/*.mocha.js",
    "travis": "npm run build-test && nyc --reporter=lcovonly mocha test/*.mocha.js && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "build": "NODE_ENV=production babel src -d dist --copy-files",
    "build-test": "BABEL_ENV=development babel src -d temp --copy-files",
    "build-dev": "BABEL_ENV=development babel src -d dist --copy-files"
  },
  "keywords": [
    "web",
    "builder",
    "modular",
    "framework",
    "koa"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kitmi/mowa.git"
  },
  "bin": {
    "mowa": "./bin/mowa.js"
  },
  "bugs": {
    "url": "https://github.com/kitmi/mowa/issues"
  },
  "license": "MIT",
  "contributors": [
    {
      "name": "Rockie Guo",
      "email": "rockie@kitmi.com.au",
      "url": "https://github.com/rockie"
    }
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-contracts": "^1.2.0",
    "babel-plugin-minify-constant-folding": "^0.3.0",
    "babel-plugin-minify-flip-comparisons": "^0.3.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-inline-consecutive-adds": "^0.3.0",
    "babel-plugin-transform-merge-sibling-variables": "^6.9.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.0",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.5.5",
    "mocha": "^3.2.0",
    "nyc": "^11.3.0",
    "should": "^8.0.2",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "archiver": "^2.1.0",
    "compare-versions": "^3.1.0",
    "escodegen": "^1.9.0",
    "inflection": "^1.12.0",
    "inquirer": "^3.0.6",
    "koa": "^2.4.1",
    "koa-better-body": "^3.0.4",
    "koa-compress": "^2.0.0",
    "koa-connect": "^2.0.1",
    "koa-convert": "^1.2.0",
    "koa-csrf": "^3.0.6",
    "koa-error": "^3.1.0",
    "koa-etag": "^3.0.0",
    "koa-flash-simple": "0.0.3",
    "koa-methodoverride": "^2.0.0",
    "koa-mount": "^3.0.0",
    "koa-router": "^7.3.0",
    "koa-session-minimal": "^3.0.4",
    "koa-static": "^4.0.2",
    "koa-views": "^6.1.3",
    "lru-cache": "^2.7.3",
    "moment": "^2.10.6",
    "moment-timezone": "^0.4.1",
    "node-ssh": "^5.1.0",
    "nodemiral": "^1.1.1",
    "randomstring": "^1.1.5",
    "rk-config": "^1.0.0",
    "rk-utils": "^2.0.2",
    "shelljs": "^0.7.7",
    "silent-npm-registry-client": "^2.1.0",
    "socket.io": "^2.0.4",
    "swig-templates": "^2.0.2",
    "tiny-xml": "^1.0.6",
    "topo-sort": "^1.0.0",
    "trie": "^0.2.1",
    "validator": "^9.4.1",
    "webpack": "^4.1.0",
    "webpack-dev-middleware": "^3.0.0",
    "webpack-merge": "^4.1.2",
    "winston": "^2.4.0",
    "winston-daily-rotate-file": "^1.7.2",
    "winstonflight": "^1.0.1"
  },
  "main": "index",
  "engines": {
    "node": "7.6.0"
  }
}
