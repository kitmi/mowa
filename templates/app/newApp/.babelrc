{
  "presets": [
    ["@babel/env", {
      "targets": {
        "node": true
      }
    }]
  ],
  "plugins": [
    ["contract", {
      "names": {
        "assert": "assert",
        "precondition": "pre",
        "postcondition": "post",
        "invariant": "invariant",
        "return": "it"
      }
    }],    
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-proposal-class-properties"
  ]
}