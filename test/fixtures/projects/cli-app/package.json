{
    "name": "cli-app",
    "version": "0.1.0",
    "description": "A new mowa project.",
    "main": "build/server.js",
    "scripts": {
        "build": "babel src -d server --copy-files && lerna exec --concurrency 1 -- npm run build",
        "build:prod": "NODE_ENV=production babel src -d server --copy-files && lerna exec --concurrency 1 -- npm run build:prod",
        "build:watch": "babel src -w -d server --copy-files",
        "postinstall": "lerna bootstrap",
        "doc": "jsdoc -c jsdoc.json -t ./node_modules/ink-docstrap/template -R README.md -d './doc' -r --verbose ./src",
        "start": "node server/start"
    },
    "keywords": [],
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "^7.1.2",
        "@babel/core": "^7.1.2",
        "@babel/plugin-proposal-class-properties": "^7.1.0",
        "@babel/plugin-proposal-decorators": "^7.1.2",
        "@babel/preset-env": "^7.1.0",        
        "babel-plugin-contract": "^1.3.0",
        "ink-docstrap": "^1.3.2",
        "jsdoc": "^3.5.5",
        "lerna": "^3.4.0",
        "mocha": "^5.2.0",
        "should": "^13.2.3"
    },
    "dependencies": {
        "mowa": "*"
    }
}
